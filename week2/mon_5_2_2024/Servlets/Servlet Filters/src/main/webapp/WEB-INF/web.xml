  </filter>
  <filter-mapping>
    <filter-name>LoggingFilter</filter-name>
    <url-pattern>/Servlet1</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>IpBlockingFilter</display-name>
    <filter-name>IpBlockingFilter</filter-name>
    <filter-class>IpBlockingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>IpBlockingFilter</filter-name>
    <url-pattern>/Servlet1</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>EnforceHttpsFilter</display-name>
    <filter-name>EnforceHttpsFilter</filter-name>
    <filter-class>EnforceHttpsFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EnforceHttpsFilter</filter-name>
    <url-pattern>/Servlet1</url-pattern>
  </filter-mapping>
</web-app>