<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

<form class="px-3 h-100">

    <div class="row">
        <h1 class="col accordion-header" id="headingOne">
            Personal
        </h1>
    </div>


    <div class="row mt-3">

        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Gender<span
                    class="text-danger">*</span></label>

            <select class="form-select" id=""
                    aria-label="Default select example" required>
                <option selected>--Select Type--</option>
                <option value="Male">Male</option>
                <option value="Female">Female</option>
                <option value="Other">Other</option>
            </select>
        </div>

        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Marriage Status</label>
            <select class="form-select" id=""
                    aria-label="Default select example">
                <option selected>--Select Type--</option>
                <option value="Married">Married</option>
                <option value="Unmarried">Unmarried</option>
                <option value="Divorsed">Divorsed</option>
            </select>
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Date of Birth<span
                    class="text-danger">*</span></label>
            <input name="" id="" type="date" class="form-control mt-2"
                   placeholder="Enter a number" required>
        </div>

    </div>

    <div class="row mt-3">

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Date of Marriage</label>
            <input name="" id="" type="date" class="form-control mt-2"
                   placeholder="Enter a number">
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Birth Place</label>
            <input name="" id="" type="text" class="form-control mt-2">
        </div>

        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Community Category<span
                    class="text-danger">*</span></label>

            <select class="form-select" id=""
                    aria-label="Default select example" required>
                <option selected>--Select Type--</option>
                <option value="Married">Married</option>
                <option value="Unmarried">Unmarried</option>
                <option value="Divorced">Divorced</option>
            </select>
        </div>

    </div>

    <div class="row mt-3">

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">UID No.</label>
            <input name="" id="" type="text" class="form-control mt-2" >
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">GPF A/C No.</label>
            <input name="" id="" type="text" class="form-control mt-2" >
        </div>

        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Religion</label>
            <select class="form-select" id=""
                    aria-label="Default select example">
                <option selected>--Select Type--</option>
                <option value="Married">Married</option>
                <option value="Unmarried">Unmarried</option>
                <option value="Divorced">Divorced</option>
            </select>
        </div>

    </div>

    <div class="row mt-3">

        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Caste</label>
            <select class="form-select" id=""
                    aria-label="Default select example">
                <option selected>--Select Type--</option>
                <option value="Married">Married</option>
                <option value="Unmarried">Unmarried</option>
                <option value="Divorced">Divorced</option>
            </select>
        </div>

        <div class="col-8 md-3 ">
            <label for="" class="mt-2">Community Category Reference</label>
            <input name="" id="" type="text" class="form-control mt-2">
        </div>

    </div>

    <div class="row mt-3">

        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Nationality</label>
            <select class="form-select" id=""
                    aria-label="Default select example">
                <option selected>--Select Type--</option>
                <option value="Married">Married</option>
                <option value="Unmarried">Unmarried</option>
                <option value="Divorsed">Divorsed</option>
            </select>
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Post Life Insurance Policy No.</label>
            <input name="" id="" type="text" class="form-control mt-2" >
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Hobbies</label>
            <input name="" id="" type="text" class="form-control mt-2" >
        </div>

    </div>

    <hr>

    <div class="row mt-3">
        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Bank Name<span
                    class="text-danger">*</span></label>

            <select class="form-select" id=""
                    aria-label="Default select example" required>
                <option selected>--Select Type--</option>
                <option value="Married">Married</option>
                <option value="Unmarried">Unmarried</option>
                <option value="Divorced">Divorced</option>
            </select>
        </div>

        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Bank Branch Name<span
                    class="text-danger">*</span></label>

            <select class="form-select" id=""
                    aria-label="Default select example" required>
                <option selected>--Select Type--</option>
                <option value="Married">Married</option>
                <option value="Unmarried">Unmarried</option>
                <option value="Divorced">Divorced</option>
            </select>
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Bank A/C Number<span class="text-danger">*</span></label>
            <input name="" id="" type="text" class="form-control mt-2" required>
        </div>
    </div>

    <div class="row mt-3">
        <div class="col-4 md-3 ">
            <label for="" class="mt-2">IFSC Code<span class="text-danger">*</span></label>
            <input name="" id="" type="text" class="form-control mt-2" required>
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">BSR Code<span class="text-danger">*</span></label>
            <input name="" id="" type="text" class="form-control mt-2" required>
        </div>

        <div class="col-4 md-3 ">

        </div>
    </div>

    <hr>

    <div class="row mt-3">
        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Passport No</label>
            <input name="" id="" type="text" class="form-control mt-2">
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Date of Expiry</label>
            <input name="" id="" type="date" class="form-control mt-2"
                   placeholder="Enter a number">
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Visa Detail</label>
            <input name="" id="" type="text" class="form-control mt-2">
        </div>
    </div>

    <div class="row mt-3">
        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Driving Licence No</label>
            <input name="" id="" type="text" class="form-control mt-2">
        </div>

        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Licence Issued for (Vehicle Type)</label>

            <select class="form-select" id=""
                    aria-label="Default select example">
                <option selected>--Select Type--</option>
                <option value="2wheeler">2-wheeler</option>
                <option value="3wheeler">3-wheeler</option>
                <option value="4wheeler">4-wheeler</option>
            </select>
        </div>

        <div class="col-4 md-3 ">
            <label for="" class="mt-2">Valid Upto</label>
            <input name="" id="" type="date" class="form-control mt-2"
                   placeholder="Enter a number">
        </div>
    </div>

    <div class="row mt-3">
        <div class="col-4 md-3">
            <label for="field11" class="mt-2 md-3">Issue State/UT</label>

            <select class="form-select" id="field11"
                    aria-label="Default select example">
                <option selected>--Select Type--</option>
                <option value="2wheeler">2-wheeler</option>
                <option value="3wheeler">3-wheeler</option>
                <option value="4wheeler">4-wheeler</option>
            </select>
        </div>

        <div class="col-8 md-3 d-flex flex-row ">

            <div class="d-flex flex-column">
                <label class="form-check-label" for="">Government <br> Vehicle Provided?</label>
                <input type="checkbox" class="form-check-input" id="">
            </div>
            <div class="d-flex flex-column">
                <label class="form-check-label" for="">Is Resident of <br> Other Country?</label>
                <input type="checkbox" class="form-check-input" id="">
            </div>
            <div class="d-flex flex-column">
                <label class="form-check-label" for="">Is Any Disciplinary <br> proceeding?</label>
                <input type="checkbox" class="form-check-input" id="">
            </div>
        </div>

    </div>

    <div class="row mt-3">
        <div class="col-8 md-3 ">
            <label for="" class="mt-2">Additional Info</label>
            <input name="" id="" type="text" class="form-control mt-2">
        </div>
    </div>

    <div class="row mt-3">

        <div class="col-4 mt-3"></div>
        <div class="col-4 mt-3"></div>

        <div class="col-4" style="margin-top: 100px">
            <div class="row">
                <input class="btn btn-secondary col-md-4" type="reset" value="Reset" style="margin: 0 15px">
                <input class="btn col-md-6" type="submit" value="Save & Continue" style=" background-color: darkorange; color: white">
            </div>
        </div>
    </div>

</form>